<div class="col-md-12 wells catogories">
    {% for categorie in categories %}
    <div class="col-md-3 col-sm-6">
        <a href="{{ categorie.url }}" title="{{ categorie.titre }}">
            <i class="fa {{ categorie.type }}"></i>
        </a>
        <h4><a href="{{ categorie.url }}" title="{{ categorie.titre }}">{{ categorie.titre }}</a></h4>
        <p>
            {{ categorie.description }}
        </p>
    </div>
    {%  endfor %}
</div>
 <div class="clearfix"></div>