<!DOCTYPE html>
<html {{ sonata_seo_html_attributes() }}>
    <head {{ sonata_seo_head_attributes() }}>
        {% block title %}{{ sonata_seo_title() }}{% endblock %}
        {{ sonata_seo_metadatas() }}
        {{ sonata_seo_link_canonical() }}
        {{ sonata_seo_lang_alternates() }}
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="icon" type="image/png" href="{{ asset('bundles/coachfront/images/favicon.ico')}}" />
        <link rel="alternate" hreflang="fr" href="http://www.particulier-coach.com" />
        {% block stylesheets %}
        <link href="{{ asset('bundles/coachfront/css/bootstrap/bootstrap.min.css')}}" rel="stylesheet">
        <link href="{{ asset('bundles/coachfront/css/styles.css')}}" rel="stylesheet">
        <link href="{{ asset('bundles/coachfront/css/responsive.css')}}" rel="stylesheet">
        <link href="{{ asset('bundles/coachfront/js/owl-carousel/owl.theme.css')}}" rel="stylesheet">
        <link href="{{ asset('bundles/coachfront/js/owl-carousel/owl.carousel.css')}}" rel="stylesheet">
        <link href="{{ asset('bundles/coachfront/css/font-awesome/css/font-awesome.min.css')}}" rel="stylesheet">
        <link href="{{ asset('bundles/coachfront/js/js-animation/animate.css')}}" rel="stylesheet">
        {% endblock %}
    </head>
    <body>
        {% block header_top %}
        {{ include('header-top.html.twig') }}
        {% endblock %}
        {% block header_nav %}
        {{ include('header-nav.html.twig') }}
        {% endblock %}
        <div class="container">           
             {% block body %}{% endblock %}            
            <!-- FOOTER -->
            {% block footer %}
            {{ include('footer.html.twig') }}
            {% endblock %}
            <!-- FOOTER -->
        </div>
        <!-- End content -->
        {% block pub %}{% endblock %}         
        {% block javascripts %}
        <!-- JQUERY PLUGINS AND MAIN JS -->
        <script src="{{ asset('bundles/coachfront/js/jquery-1.11.1.min.js')}}"></script>
        <script src="{{ asset('bundles/coachfront/js/js-bootstrap/bootstrap.min.js')}}"></script>
        <script src="{{ asset('bundles/coachfront/js/main.js')}}"></script>
         <!--Animation plugin-->
        <script src="{{ asset('bundles/coachfront/js/js-animation/wow.min.js')}}"></script>        
        <script src="{{ asset('bundles/coachfront/js/js-typed/typed.js')}}" ></script>
        <!-- OWL carroussel-->
        <script src="{{ asset('bundles/coachfront/js/owl-carousel/owl.carousel.min.js')}}" ></script>
        <script src="{{ asset('bundles/coachfront/js/init.js')}}" ></script>
        <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
        <script src="{{ asset('bundles/coachfront/js/ie/ie10-viewport-bug-workaround.js')}}"></script>
